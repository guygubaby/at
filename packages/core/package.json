{
  "name": "@bryce-loskie/at",
  "version": "0.1.0",
  "packageManager": "pnpm@6.32.3",
  "description": "animate.css powered vue transition component",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "scripts": {
    "prepublishOnly": "pnpm build",
    "dev": "pnpm build -- --watch",
    "build": "vueup",
    "start": "esno src/index.ts"
  },
  "funding": "https://github.com/sponsors/guygubaby",
  "author": "guygubaby <guygubaby@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/guygubaby/at/issues"
  },
  "homepage": "https://github.com/guygubaby/at#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guygubaby/at.git"
  },
  "keywords": [
    "vue transition"
  ],
  "devDependencies": {
    "@bryce-loskie/vueup": "^1.0.13"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "animate.css": "^4.1.1"
  }
}
